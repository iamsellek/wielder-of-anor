{
  "name": "wielder-of-anor",
  "version": "1.2.2",
  "description": "A gatekeeper that protects your remote git repo.",
  "main": "/lib/index.js",
  "types": "/lib/index.d.ts",
  "bin": {
    "wielder-of-anor": "./lib/index.js"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/iamsellek/wielder-of-anor"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "test": "jest && npm run build && npm i -g",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "prepublish": "npm test && npm run build"
  },
  "keywords": [
    "gatekeeper",
    "swearing",
    "debugger",
    "console",
    "logs",
    "protect",
    "repo",
    "prevent"
  ],
  "author": "Chris Sellek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iamsellek/wielder-of-anor/issues"
  },
  "homepage": "https://github.com/iamsellek/wielder-of-anor#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "clear": "^0.1.0",
    "commander": "^8.2.0",
    "jest": "^27.2.1",
    "simple-git": "^3.16.0",
    "ts-jest": "^27.0.5"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/clear": "^0.1.2",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.6",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
